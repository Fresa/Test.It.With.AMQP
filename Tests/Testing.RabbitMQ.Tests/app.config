<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="Logging" type="Log.It.LoggingSection, Log.It" />
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <Logging Factory="Log.It.With.NLog.NLogLoggerFactory, Log.It.With.NLog" />
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.0.0" newVersion="3.1.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="Test.It.With.RabbitMQ.Tests" />
    </extensions>
    <variable name="pipeSeparatorLayout" value="${date:format=yyyy-MM-dd HH\:mm\:ss.fff} | ${machinename} | ${logger} | ${level} | ${ltc:item=Messaging.MessageId} | ${ltc:item=Messaging.CorrelationId} | ${ltc:item=Messaging.MessageKey} | ${ltc:item=Messaging.UserId} | ${ltc:item=AggregateId} | ${ltc:item=TenantId} | ${ltc:item=Aggregate} | ${messageLayout} ${onexception:| ${exception:format=type} | ${exception:format=method} | ${exception:format=message} | ${exception:format=stacktrace} | ${exception:method:maxInnerExceptionLevel=5:innerFormat=shortType,message,method}}" />
    <variable name="simplePipeSeparatorLayout" value="${date:format=yyyy-MM-dd HH\:mm\:ss.fff} | ${logger} | ${level} | ${message}" />
    <targets>
      <target name="XUnit2" xsi:type="XUnit2" layout="${simplePipeSeparatorLayout}" />
    </targets>
    <rules>
      <logger name="*" minLevel="Trace" writeTo="XUnit2" />
    </rules>
  </nlog>
</configuration>